#!/bin/sh

cd ${0%/*} || exit 1    # run from this directory

makeType=${1:-libso}
set -x

./src/Allwmake

wmake  ./applications/utilities/preProcessing/canteraToFPVFoamV30
wmake  ./applications/utilities/postProcessing/FPVFoamV30Post
wmake  ./applications/solver/FPVFoamV30

# ----------------------------------------------------------------- end-of-file
